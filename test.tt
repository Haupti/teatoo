mem:{
    IF (EMPTY?) (
        PUT [01101000] -- h
        PUT [01100101] -- e
        PUT [01101100] -- l
        PUT [01101100] -- l
        PUT [01101111] -- o
        PUT [00100000] -- \s
        PUT [01110111] -- w
        PUT [01101111] -- o
        PUT [01110010] -- r
        PUT [01101100] -- l
        PUT [01100100] -- d
        RETURN [1]
    )
    RETURN (TAKE)
}

run_hello_world:{
    OUTCHAR (EXEC (STACK $mem [01101000]))
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
    OUTCHAR (EXEC mem)
}

run_hello_world_twice:{
    EXEC $run_hello_world;
    OUTCHAR [00001010];
    EXEC $run_hello_world;
}

EXEC $run_hello_world_twice;
