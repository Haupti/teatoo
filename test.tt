        PUT [01100101] -- e
        PUT [01101100] -- l
        PUT [01101100] -- l
        PUT [01101111] -- o
        PUT [00100000] -- \s
        PUT [01110111] -- w
        PUT [01101111] -- o
        PUT [01110010] -- r
        PUT [01101100] -- l
        PUT [01100100] -- d
        RETURN [1]
